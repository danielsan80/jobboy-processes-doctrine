{
    "name": "dansan/jobboy-processes-doctrine",
    "description": "Doctrine ProcessRepository for dansan/jobboy",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Danilo Sanchi",
            "email": "danilo.sanchi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "dansan/jobboy": "^0.2.0"
    },
    "require-dev": {
        "dansan/jobboy-test-pack":"*",
        "doctrine/dbal": "~2.4",
        "dansan/fixture-handler": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "process-timeout": 60,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JobBoy\\Process\\": "src/JobBoy/Process/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },

    "minimum-stability": "stable"
}
